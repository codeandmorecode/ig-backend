<p id="notice"><%= notice %></p>

<h1>Photos</h1>

<table>
  <thead>
    <tr>
      <th>Photo_id</th>
      <th>Country</th>
      <th>State</th>
      <th>City</th>
      <th>Landmark</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Perspective</th>
      <th>User_id</th>
      <th>Photo_url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.photo_id %></td>
        <td><%= photo.country %></td>
        <td><%= photo.state %></td>
        <td><%= photo.city %></td>
        <td><%= photo.landmark %></td>
        <td><%= photo.latitude %></td>
        <td><%= photo.longitude %></td>
        <td><%= photo.perspective %></td>
        <td><%= photo.user_id %></td>
        <td><%= photo.photo_url %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
